<form id="deepfried" action="/play" method="post">
    <input type="text" name="skin" value="deepfried" style="display:none;">
</form>
<form id="default" action="/play" method="post">
    <input type="text" name="skin" value="default" style="display:none;">
</form>
<form id="whitebear" action="/play" method="post">
    <input type="text" name="skin" value="whitebear" style="display:none;">
</form>
<form id="miñor" action="/play" method="post">
    <input type="text" name="skin" value="miñor" style="display:none;">
</form>
<form id="miñand" action="/play" method="post">
    <input type="text" name="skin" value="miñand" style="display:none;">
</form>
<form id="minor" action="/play" method="post">
    <input type="text" name="skin" value="minor" style="display:none;">
</form>
<form id="minand" action="/play" method="post">
    <input type="text" name="skin" value="minand" style="display:none;">
</form>
<form id="miñan420" action="/play" method="post">
